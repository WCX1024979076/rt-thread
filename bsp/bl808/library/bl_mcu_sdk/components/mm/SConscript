src = Split('''
    mem.c
    mmheap/mmheap.c
    mmheap/bflb_mmheap.c
''')

path = [cwd,cwd + '/mmheap' ]

libpath = []
libs = []

group = DefineGroup('Components_Mem', src, depend = [''], CPPPATH = path, LIBS = libs, LIBPATH = libpath)

